<% include ./partials/header %>
<div class="ui main container">
    <h1>Index page</h1>

    <% blogs.forEach((blog)=>{ %>
        <div>
            <h2><%=blog.title%></h2>
            <img src="<%=blog.image%>" style="width: 500px">
            <span><%=blog.created%></span>
            <p><%=blog.body.substring(0,100)%>...</p>
            <a href="/blogs/<%=blog._id%>">Read More...</a>
        </div>
    <% }); %>
</div>
<% include ./partials/footer %>